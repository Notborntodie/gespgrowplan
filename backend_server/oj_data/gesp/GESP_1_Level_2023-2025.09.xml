            <solution language="C++"><![CDATA[#include<bits/stdc++.h>
using namespace std;
int c(int n){
	if(n==1){
		return 1;
	}
	return n+c(n-1);
}
int main(){
	int cnt = 0,n;
	cin>>n;
	for(int i=1;i<=n;i++){

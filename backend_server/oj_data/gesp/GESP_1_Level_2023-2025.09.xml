            <solution language="C++"><![CDATA[#include<bits/stdc++.h>
using namespace std;
int main(){
	int h,m,s,k;
	cin>>h>>m>>s>>k;
	if(s+k>=60){
		s = s+k-60;
		m++;
	}
	else s = s+k;
	if(m>=60){
		m = m-60;
		h++;
	}
	cout<<h<<' '<<m<<' '<<s;
	return 0;
}]]></solution>
    
